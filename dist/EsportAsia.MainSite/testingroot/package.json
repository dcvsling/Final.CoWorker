{
    "name": "limit-game",
    "version": "1.0.0",
    "license": "MIT",
    "angular-cli": {},
    "scripts": {
        "start": "ng serve",
        "build": "ng build --prod --aot --output-hashing=none",
        "devbuild": "ng build",
        "lint": "tslint \"Dev/**/*.ts\"",
        "test": "ng test",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor",
        "bundle": "browserify -s main Dev/Client/main.js > dist/bundle.js",
        "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
        "build:aot": "ngc -p Dev/Client/tsconfig-aot.json && rollup -c Dev/Client/rollup-config.js",
        "serve:aot": "lite-server -c Dev/Client/bs-config.aot.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "4.1.0",
        "@angular/cli": "1.3.0",
        "@angular/common": "4.1.0",
        "@angular/compiler": "4.1.0",
        "@angular/compiler-cli": "4.1.0",
        "@angular/core": "4.1.0",
        "@angular/forms": "4.1.0",
        "@angular/http": "4.1.0",
        "@angular/material": "^2.0.0-beta.3",
        "@angular/platform-browser": "4.1.0",
        "@angular/platform-browser-dynamic": "4.1.0",
        "@angular/platform-server": "4.1.0",
        "@angular/router": "4.1.0",
        "core-js": "^2.4.1",
        "ng-inline-svg": "^3.2.0",
        "ng-recaptcha": "^2.1.0",
        "ng2-cache": "^0.1.11",
        "ng2-lazyload-image": "^2.4.0",
        "ngx-bootstrap": "^1.6.5",
        "ngx-clipboard": "^5.1.0",
        "ngx-cookie": "^1.0.0",
        "primeng": "^4.0.3",
        "rxjs": "^5.3.0",
        "social-share-ng2": "^0.3.1",
        "social-share.js": "^1.0.16",
        "ts-helpers": "^1.1.2",
        "typescript": "^2.4.1",
        "zone.js": "^0.8.5"
    },
    "devDependencies": {
        "@angular/cli": "1.3.0",
        "@angular/compiler-cli": "4.1.0",
        "@types/core-js": "^0.9.41",
        "@types/jasmine": "^2.5.47",
        "@types/node": "^7.0.12",
        "browserify": "^14.3.0",
        "codelyzer": "^2.1.1",
        "jasmine-core": "2.5.2",
        "jasmine-spec-reporter": "2.5.0",
        "karma": "1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-remap-istanbul": "^0.2.1",
        "protractor": "4.0.9",
        "rollup": "^0.41.6",
        "rollup-plugin-commonjs": "^8.0.2",
        "rollup-plugin-node-resolve": "^2.0.0",
        "ts-node": "^3.2.0",
        "tslint": "^4.0.2",
        "typescript": "^2.4.1",
        "uglify-js": "^2.8.29",
        "uglifyjs": "^2.4.10",
        "webdriver-manager": "12.0.4"
    }
